{
  "name": "mini-seed",
  "version": "1.0.0",
  "private": true,
  "description": "mini-seed12",
  "templateInfo": {
    "name": "default",
    "typescript": true,
    "css": "less"
  },
  "scripts": {
    "start:dev": "node scripts/prebuild dev && npm run dev:weapp",
    "build:dev": "node scripts/prebuild dev && npm run build:weapp",
    "start:test": "node scripts/prebuild test && npm run dev:weapp",
    "build:test": "node scripts/prebuild test && npm run build:weapp",
    "start:pre": "node scripts/prebuild pre && npm run dev:weapp",
    "build:pre": "node scripts/prebuild pre && npm run build:weapp",
    "start:prod": "node scripts/prebuild prod && npm run dev:weapp",
    "build:prod": "node scripts/prebuild prod && npm run build:weapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "build:weapp": "taro build --type weapp",
    "lint": "eslint --ext .js,.ts src",
    "test": "npm run lint",
    "postpublish": "npm run ci & node ./scripts/notice/index.js",
    "ci": "mp-ci preview ./ --pkp=./private.key"
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "",
  "dependencies": {
    "@babel/runtime": "^7.17.9",
    "@taroify/core": "0.0.29-alpha.8",
    "@tarojs/cli": "3.4.8",
    "@tarojs/components": "3.4.8",
    "@tarojs/plugin-framework-react": "^3.4.8",
    "@tarojs/react": "3.4.8",
    "@tarojs/router": "3.4.8",
    "@tarojs/runtime": "3.4.8",
    "@tarojs/taro": "3.4.8",
    "async-validator": "4.1.1",
    "babel-plugin-import": "^1.13.5",
    "bankcardinfo": "^2.0.6",
    "dayjs": "^1.11.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "mobx": "4.8.0",
    "mobx-react": "6.1.4",
    "prop-types": "^15.8.1",
    "react": "^17.0.0",
    "react-dom": "^17.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@tarojs/mini-runner": "3.4.8",
    "@tarojs/webpack-runner": "3.4.8",
    "@types/react": "^18.0.8",
    "@types/webpack-env": "^1.16.4",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "babel-preset-taro": "3.4.8",
    "cross-env": "7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^16.0.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-config-taro": "3.3.18",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "mp-ci": "^1.2.1",
    "stylelint": "9.3.0",
    "typescript": "^4.6.3"
  }
}
